{
	"Name": "simoou",
	"Start": "Simoou.exe",
	"Source": "..\\simoou\\Simoou\\bin\\Debug",
	"Filter": [ ".exe", ".dll" ],
	"DesktopIcon": true,
	"StartScreen": true,
	"InstallFolder": "C:\\Program Files",
	"OverrideExists": true,
	"Output": "simoou-installer.exe",
	"Uninstaller":"uninstaller.exe",
	"Icon":"favicon.ico",
	"RegValues": [
		{
			"Type": "HKEY_LOCAL_MACHINE",
			"Path": "SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Uninstall",
			"Name": "simoou",
			"Values": {
				"DisplayIcon": "${install_path}\\simoou.exe",
				"InstallLocation":"${install_path}",
				"DisplayName": "simoou",
				"DisplayVersion": "1.2.0",
				"Publisher":"tqz",
				"EstimatedSize":"${install_size}",
				"UninstallString": "${install_path}\\${uninstaller_file}",
				"NoModify":"1",
				"NoRepair":"1",
				"sEstimatedSize2":"${install_size}",
				"VersionMajor":"1",
				"VersionMinor":"2"
			}
		}
	],
	"AfterInstall":"",
	"AfterRemove":""
}